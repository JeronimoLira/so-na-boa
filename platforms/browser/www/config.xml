<?xml version='1.0' encoding='utf-8'?>
<widget id="com.atlanteam.sonaboa" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>SO NA BOA</name>
    <description>
          Aplicativo de integração de pessoas em baladas, bares e restaurantes
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
       Atlanteam
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*.google.com" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <access origin="http://sonaboa.net" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />
    <allow-intent href="http://sonaboa.net/*" />
    <preference name="android-build-tool" value="ant" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="orientation" value="portrait" />
</widget>
